<!DOCTYPE html>
<html>
    <head>
        <meta name="robots" content="noindex, nofollow">
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {# <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}" /> #}

        <title>{% block title %}Stories{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.8.0/css/bulma.min.css"></link>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <noscript>Your browser does not support JavaScript!</noscript>

        <input id="g-recaptcha" data-action="game" name="g-recaptcha-response" hidden value=""></input>
        {% block javascripts %}
            {# <script src="https://cdn.jsdelivr.net/npm/vue@2.6.0"></script> PROD #}
            <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
            <script defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/vee-validate@latest/dist/vee-validate.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.min.js"></script>
        {% endblock %}
    </body>
</html>
